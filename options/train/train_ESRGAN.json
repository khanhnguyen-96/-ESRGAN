{
  "name": "RRDB_ESRGAN_x4_Flickr2K" //  please remove "debug_" during training
  ,
  "use_tb_logger": true,
  "model": "srragan",
  "scale": 4,
  "gpu_ids": [
    0
  ],
  "datasets": {
    "train": {
      "name": "Flickr2K",
      "mode": "LRHR",
      "dataroot_HR": "/content/gdrive/My Drive/LVTN/SuperResolution/ESRGAN/Dataset/Flickr2K_HR.lmdb",
      "dataroot_LR": "/content/gdrive/My Drive/LVTN/SuperResolution/ESRGAN/Dataset/Flickr2K_LR_unknown_X4.lmdb",
      "subset_file": null,
      "use_shuffle": true,
      "n_workers": 1,
      "batch_size": 16,
      "HR_size": 128,
      "use_flip": true,
      "use_rot": true
    },
    "val": {
      "name": "PIRM_self_val",
      "mode": "LRHR",
      "dataroot_HR": "/content/gdrive/My Drive/LVTN/SuperResolution/ESRGAN/Dataset/PIRM_Self-Val_set_HR.lmdb",
      "dataroot_LR": "/content/gdrive/My Drive/LVTN/SuperResolution/ESRGAN/Dataset/PIRM_Self-Val_set_LR_4x.lmdb"
    }
  },
  "path": {
    "root": "/content/gdrive/My Drive/LVTN/SuperResolution/ESRGAN/-ESRGAN"
    // ,
    // "resume_state": "experiments/RRDB_ESRGAN_x4_celeba_val_1000/training_state/1.state"
    ,
    "pretrain_model_G": null
  },
  "network_G": {
    "which_model_G": "RRDB_net" // RRDB_net | sr_resnet
    ,
    "norm_type": null,
    "mode": "CNA",
    "nf": 64,
    "nb": 23,
    "in_nc": 3,
    "out_nc": 3,
    "gc": 32,
    "group": 1,
    "scale": 4
  },
  "network_D": {
    "which_model_D": "discriminator_vgg_128",
    "norm_type": "batch",
    "act_type": "leakyrelu",
    "mode": "CNA",
    "nf": 64,
    "in_nc": 3,
    "scale": 4
  },
  "train": {
    "lr_G": 1e-4,
    "weight_decay_G": 0,
    "beta1_G": 0.9,
    "lr_D": 1e-4,
    "weight_decay_D": 0,
    "beta1_D": 0.9,
    "lr_scheme": "MultiStepLR",
    "lr_steps": [
      50000,
      100000,
      200000,
      300000
    ],
    "lr_gamma": 0.5,
    "pixel_criterion": "l1",
    "pixel_weight": 1e-2,
    "feature_criterion": "l1",
    "feature_weight": 1,
    "gan_type": "vanilla",
    "gan_weight": 5e-3
    //for wgan-gp
    // , "D_update_ratio": 1
    // , "D_init_iters": 0
    // , "gp_weigth": 10
    ,
    "manual_seed": 0,
    "niter": 5e5,
    "val_freq": 5e3
  },
  "logger": {
    "print_freq": 200,
    "save_checkpoint_freq": 5e3
  }
}